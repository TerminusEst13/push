actor DroneBall : ArachnotronPlasma
{
    Speed 35
    Damage (10)
    ReactionTime 50
    DamageType "DroneDamage"
    +SEEKERMISSILE
    +FORCEXYBILLBOARD
    States
    {
    Spawn:
        APLS AB 5 Bright A_SeekerMissile(10,15)
        TNT1 A 0 A_Countdown
        Loop
    Death:
        TNT1 A 0
        TNT1 A 0 A_SpawnItemEx("DroneJumpage",0,0,0,0,0,0,0,32)
        goto Super::Death
    }
}

