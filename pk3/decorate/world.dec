actor HitlerBall : CacodemonBall
{
    Radius 12
    Height 8
    ReactionTime 100
    DamageType "HitlerDamage"
    States
    {
    Spawn:
        BAL2 AB 4 Bright
        TNT1 A 0 A_Countdown
        Loop
    Death:
        TNT1 A 0
        TNT1 A 0 A_SpawnItemEx("HitlerJumpage",0,0,0,0,0,0,0,32)
        BAL2 A 1 Bright
        goto Super::Death
    }
}

