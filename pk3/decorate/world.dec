// Push 17

Actor Cacopusher : Cacodemon
{
    Radius 24
    Height 32
    Scale 0.5
    Health 500
    DamageFactor "NoDamage", 0.0
    -SOLID
    +MISSILEMORE
    +LOOKALLAROUND
    +QUICKTORETALIATE
    SeeSound "babbycaco/see"
    ActiveSound "babbycaco/idle"
    PainSound "babbycaco/pain"
    DeathSound "babbycaco/death"
    States
    {
    Spawn:
        HEAD A 35
    Spawn2:
        HEAD A 3 A_Wander
        TNT1 A 0 A_Look
        Loop
    See:
        HEAD A 3 A_Chase
        HEAD A 3 A_FastChase
        Loop
    Missile:
        HEAD BC 5 A_FaceTarget
        HEAD D 5 Bright A_CustomMissile("HitlerBall",32,0)
        TNT1 A 0 A_Jump(16,"ClearTarget")
        goto See
    Death:
        HEAD F 2 BRIGHT
        TNT1 A 0 A_Gravity
        TNT1 A 0 A_Scream
        TNT1 AAAAAAAAAAAAAAA 0 A_SpawnItemEx("RedDeathSpark",random(-10,10),random(-10,10),random(24,42),random(-150,150)/100.0,random(-150,150)/100.0,random(100,300)/300.0,random(-180,180),161)
        TNT1 AAAAAAAAA 0 A_SpawnItemEx("Binary0",random(-10,10),random(-10,10),random(24,42),random(-100,100)/150.0,random(-100,100)/150.0,random(100,300)/250.0,random(-180,180),160)
        TNT1 AAAAAAAAA 0 A_SpawnItemEx("Binary1",random(-10,10),random(-10,10),random(24,42),random(-100,100)/150.0,random(-100,100)/150.0,random(100,300)/250.0,random(-180,180),160)
        HEAD FFF 2 A_FadeOut(0.1)
        TNT1 A 0 A_NoBlocking
        HEAD FFF 2 A_FadeOut(0.1)
        HEAD FFF 2 A_FadeOut(0.1)
        PLAY Z 1
        Stop
    Pain:
        HEAD E 3
        HEAD E 3 A_Pain
        HEAD F 6
    ClearTarget:
        TNT1 A 0 A_ClearTarget
        HEAD AAAAAAAAAAAAAAAA 3 A_Wander
        TNT1 A 0 A_ClearTarget
        goto Spawn2
    }
}

actor RedDeathSpark : DeathSpark
{
    Scale 0.2
    Translation "112:127=168:183"
}

