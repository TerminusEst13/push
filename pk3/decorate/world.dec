ACTOR SuddenDeathDrone
{
    Health 0x7FFFFFFF
    Radius 16
    Height 56
    Mass 200
    PainChance 255
    Speed 20
    Monster
    +FLOAT
    +NOGRAVITY
    +FLOATBOB
    +MISSILEMORE
    +MISSILEEVENMORE
    +LOOKALLAROUND
    +NOINFIGHTING
    SeeSound "drone/see"
    ActiveSound "drone/idle"
    Obituary "%o shouldn't have died to this."
    States
    {
    Spawn:
        DRON A 5
    Spawn2:
        DRON A 3 A_Wander
        TNT1 A 0 A_Look
        Loop
    See:
        DRON A 3 A_Chase
        DRON A 3 A_FastChase
        TNT1 A 0 A_Jump(16,"ClearTarget")
        Loop
    Missile:
        DRON BC 2 A_FaceTarget
        DRON D 2 Bright A_CustomMissile("DroneBall",32,0,0,0,0)
        DRON A 10
        TNT1 A 0 A_Jump(16,"ClearTarget")
        Goto See
    Death:
        TNT1 A 0
        TNT1 A 1 A_NoBlocking
        TNT1 A -1
        Stop
    ClearTarget:
    Pain:
        TNT1 A 0 A_ClearTarget
        DRON AAAAAAAAAAAAAAAA 3 A_Wander
        TNT1 A 0 A_ClearTarget
        goto Spawn2
    }
}