ACTOR FlameJet 30001 //YOU GOT THE TORCH, YOU GOT THE POWER! YEAH!
{
    Radius 8
    Height 15
    Scale 0.8
    Mass 0x7FFFFFF
    -PUSHABLE
    +SOLID
    +RANDOMIZE
    +FORCEYBILLBOARD
    DamageType "NoDamage"
    States
    {
    Spawn:
        TNT1 A 0 A_PlaySound("FlameJet/Torch")
        FJUP ABCD 2
        FJUP EFGHIJKL 2 Bright A_Explode(512,128)
        FJUP MNOP 2 Bright
        FJUP A 70
        Loop
    }
}

