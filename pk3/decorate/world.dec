Actor KD_Meteor
{
    Projectile
    +FORCEXYBILLBOARD
    
    RenderStyle Add
    Alpha 0.99
    Scale 0.4
    Damage 4
    Speed 20
    Radius 12
    Height 24
    SeeSound "Imp/Attack"
    DeathSound "Weapons/RockLX"
    DamageType "GrenadeDamage"
    Decal "Scorch"
    States
    {
        Spawn:
            METR A 1 Bright A_SpawnItemEx("KD_MeteorHalo", 0, 0, 0, 0, 0, 0, 0, SXF_CLIENTSIDE, 0)
            //TNT1 A 0 A_SpawnItemEx("KD_MeteorGlow", 0, 0, 0, 0, 0, 0, 0, SXF_CLIENTSIDE, 0)
            TNT1 AA 0 A_SpawnItemEx("KD_MeteorFlame", random(-400,400)/100.00, random(-400,400)/100.00, random(-400,400)/100.00, Random(0,4)/10.0 * MomX, Random(0,4)/10.0 * MomY, Random(8, 10)/10.0 * 2.0, 0, SXF_CLIENTSIDE | SXF_ABSOLUTEMOMENTUM, 30)
            TNT1 A 0 A_SpawnItemEx("KD_MeteorEmber", Random(-150, 150)/10.0, Random(-150, 150)/10.0, Random(-150, 150)/10.0, Random(2, 4)/10.0 * MomX + Random(-25, 25)/10.0, Random(2,4)/10.0 * MomY + Random(-25, 25)/10.0, Random(3,15/10.0), 0, SXF_CLIENTSIDE | SXF_ABSOLUTEMOMENTUM, 130)
            //TNT1 A 0 A_SpawnItemEx("KD_MeteorSmoke", Random(-150, 150)/10.0, Random(-150, 150)/10.0, Random(-150, 150)/10.0, 0, 0, Random(3,15/10.0), 0, SXF_CLIENTSIDE | SXF_ABSOLUTEMOMENTUM, 130)
            Loop
        Death:
            TNT1 A 2
            Stop
    }
}

Actor KD_MeteorBase
{
    +NOINTERACTION
    +FORCEXYBILLBOARD
    
    RenderStyle Add
}

Actor KD_MeteorFlame : KD_MeteorBase
{
    Scale 0.5
    Alpha 0.0
    States
    {
        Spawn:
            TNT1 A 1
            TNT1 AA 0 A_Jump(256, "SpriteA", "SpriteB", "SpriteC", "SpriteD")
            Loop
        SpriteA:
            RABF A 1 Bright A_FadeIn(0.2)
            TNT1 A 0 A_JumpIf(Alpha >= 0.8, "OutA")
            Loop
        SpriteB:
            RABF B 1 Bright A_FadeIn(0.2)
            TNT1 A 0 A_JumpIf(Alpha >= 0.8, "OutB")
            Loop
        SpriteC:
            RABF C 1 Bright A_FadeIn(0.2)
            TNT1 A 0 A_JumpIf(Alpha >= 0.8, "OutC")
            Loop
        SpriteD:
            RABF D 1 Bright A_FadeIn(0.2)
            TNT1 A 0 A_JumpIf(Alpha >= 0.8, "OutD")
            Loop
        OutA:
            RABF A 1 Bright A_FadeOut(random(5,10)/10.00)
            TNT1 A 0 A_ChangeVelocity(MomX * 0.98, MomY * 0.98, MomZ + Random(5,15)/100.00, CVF_REPLACE)
            Loop
        OutB:
            RABF B 1 Bright A_FadeOut(random(5,10)/10.00)
            TNT1 A 0 A_ChangeVelocity(MomX * 0.98, MomY * 0.98, MomZ + Random(5,15)/100.00, CVF_REPLACE)
            Loop
        OutC:
            RABF C 1 Bright A_FadeOut(random(5,10)/10.00)
            TNT1 A 0 A_ChangeVelocity(MomX * 0.98, MomY * 0.98, MomZ + Random(5,15)/100.00, CVF_REPLACE)
            Loop
        OutD:
            RABF D 1 Bright A_FadeOut(random(5,10)/10.00)
